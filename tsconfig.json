{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "references": [{ "path": "packages/website" }],
  "compilerOptions": {
    "rootDir": ".",
    "noEmit": true,
    "erasableSyntaxOnly": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "plugins": [
      {
        "name": "@effect/language-service",
        "namespaceImportPackages": []
      }
    ]
  }
}
