{
  "name": "urbanfaubion.com",
  "description": "A personal website",
  "version": "1.0.0",
  "author": "Urban Faubion <urban.faubion@gmail.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebuild": "npm run clean",
    "clean": "rm -rf public/",
    "copy": "cp static/CNAME public/",
    "deploy": "gatsby build --prefix-links",
    "predeploy": "npm run test",
    "postdeploy": "gh-pages -d public -b master",
    "dev": "gatsby develop",
    "typecheck": "glow --include-warnings",
    "lint": "eslint --format codeframe './{components,pages,static}/**/*.{js,json}'",
    "fix": "prettier --write '{components,pages,static}/**/*.{js,json,md}'",
    "test": "npm run lint && npm run flow"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "dependencies": {
    "front-matter": "^2.0.8",
    "gatsby": "^0.12.0",
    "history": "^2.1.1",
    "markdown-it": "^6.0.2",
    "moment": "^2.14.1",
    "ramda": "^0.22.1",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.3.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.0",
    "react-ga": "^2.1.2",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "8.2.1",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "4.17.0",
    "eslint-config-babel": "7.0.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-jest": "21.9.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.6.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.36.0",
    "gh-pages": "^0.11.0",
    "glow": "1.2.2",
    "image-webpack-loader": "^2.0.0",
    "null-loader": "^0.1.1",
    "prettier": "1.10.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
